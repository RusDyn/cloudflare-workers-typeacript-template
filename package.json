{
  "name": "cloudflare-workers-typeacript-template",
  "version": "1.0.0",
  "description": "TypeScript template for Cloudflare Workers optimized for AI tools",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "tsc",
    "lint": "eslint --ext .ts --ext .js src/",
    "lint:fix": "eslint --ext .ts --ext .js src/ --fix",
    "format": "prettier --write \"**/*.ts\" \"**/*.js\" \"**/*.json\" \"**/*.md\"",
    "prepare": "husky install"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "typescript",
    "template",
    "ai-tools"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250419.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.25.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-unicorn": "^58.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-toml": "^2.0.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1",
    "vitest-environment-miniflare": "^2.14.4",
    "wrangler": "^4.12.0"
  }
}
